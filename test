digraph {
  node[fontsize=6];
  edge[fontsize=6, arrowsize=0.6];
  0 [label="4: 4 [r/4, n/4, x/4] False"];
  1 [label="5: 5 [r/5, n/5, x/5] True"];
  m_0 [shape=point, width=0.00001,height=0.00001];
  m_0 -> 0 [label="(#n/4 <= 0) && (#r/4 = #x/4)"];
  0 -> 0 [label="((not #n/4 <= 0)) && ((((#n/4 - 1) = n/4) && ((#x/4 + #n/4) = x/4)) && (#r/4 = r/4))", style=dashed];
  0 -> 1 [label="(r/4 = #r/5) && ((n/4 = #n/5) && (x/4 = #x/5))"];
}